using UnityEngine;
[CreateAssetMenu(fileName = "Line", menuName = "Scriptable Objects/Line", order=0)]
public class Line : ScriptableObject
{
    [SerializeField] private string _line;
    [SerializeField] private string[] _possibleLines;
    
    public string GetLine => _line;
    
    public string[] GetAllLines => _possibleLines;
}
